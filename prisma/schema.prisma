generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id         BigInt  @id @default(autoincrement())
  email      String  @unique(map: "email") @db.VarChar(255)
  winnings   Int?    @default(0)
  picture_id String? @db.Text
}
